{
    "manifest_version": 2,
    "name": "Quick Tools (QT)",
    "description": "Open ICM/case/ASC or Wawsobserver/Applens v2/v3",
    "version": "2.0",
    "author": "Vijay Saayi",
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Q",
                "windows": "Ctrl+Q",
                "mac": "Command+Q",
                "chromeos": "Ctrl+Q",
                "linux": "Ctrl+Q"
            }
        }
    },
    "browser_action": {
        "default_icon": {
            "256": "icon.png"
        },
        "default_popup": "popup.html",
        "default_title": "Handy Tool for opening Cases/ related tools"
    },
    "permissions": [
        "activeTab",
        "contextMenus"
    ],
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "icons": {
        "16": "icon.png"
        
     }
}